{
  "name": "inspire-me",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "moment": "^2.23.0",
    "netlify-lambda": "^1.1.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "unfurled": "^1.1.5"
  },
  "scripts": {
    "start": "run-p start-**",
    "start-app": "react-scripts start",
    "start-functions": "netlify-lambda serve ./src/server/functions",
    "build": "yarn build-functions && yarn build-app",
    "buildp": "run-p build-**",
    "build-app": "react-scripts build",
    "build-functions": "netlify-lambda build ./src/server/functions",
    "dock-build": "docker build --tag $DOCKER_REPO:$TAG_NAME ./",
    "dock-run": "docker run -p 9000:9000 $DOCKER_REPO:$TAG_NAME",
    "dock-deploy": "docker push $DOCKER_REPO:$TAG_NAME",
    "kube-takeoff": "NAMESPACE=inspire-me IMAGE_TAG=$DOCKER_REPO:$TAG_NAME PORT=9000 POD_NAME=inspire-me-pod SERVICE_NAME=inspire-me-service ./bin/kube-launch.sh",
    "kube-shutdown": "NAMESPACE=default SERVICE_NAME=inspire-me-service ./bin/kube-destroy.sh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "standard-version"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-loader": "^8.0.4",
    "http-proxy-middleware": "^0.19.1",
    "npm-run-all": "^4.1.5",
    "standard-version": "^4.4.0"
  }
}
